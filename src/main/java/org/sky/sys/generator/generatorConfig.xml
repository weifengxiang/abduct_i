<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="F:\zz.maven\local\repo\mysql\mysql-connector-java\5.1.39\mysql-connector-java-5.1.39.jar" />
	<context id="DB2Tables"  targetRuntime="MyBatis3">
		<!-- 这里引入扩展分页插件 -->   
    	<plugin type="org.sky.sys.generator.plugin.MySqlPaginationPlugin" />  
    	<!-- 综合查询查询 -->
    	<plugin type="org.sky.sys.generator.plugin.IntegratedQueryPlugin" />
		<commentGenerator>  
            <property name="suppressDate" value="true"/>  
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->  
            <property name="suppressAllComments" value="true"/>  
        </commentGenerator>  
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"			
						connectionURL="jdbc:mysql://localhost:3306/abduct"			
						userId="root" password="root" />
		<!-- 生成模型的包名和位置-->  
		<javaModelGenerator targetPackage="org.sky.msg.model" targetProject="src\main\java" />
		<!-- 生成映射文件的包名和位置--> 
		<sqlMapGenerator targetPackage="org.sky.msg.sqlmap" targetProject="src\main\java" />
		<!-- 生成DAO的包名和位置--> 
		<javaClientGenerator targetPackage="org.sky.msg.client"	targetProject="src\main\java" type="XMLMAPPER" />
		<table tableName="TB_ST_MSG"></table>
